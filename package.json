{
  "name": "@graphql-otel/monorepo",
  "version": "0.0.0-alpha.37",
  "private": true,
  "scripts": {
    "dev": "DEBUG=\"@graphql-debugger:*\" TRACE_SCHEMA=\"true\" turbo dev --filter \"graphql-debugger\"",
    "build": "turbo build",
    "test": "turbo test",
    "clean": "find . -type d \\( -name \"node_modules\" -o -name \"build\" \\) -exec rm -rf {} +"
  },
  "devDependencies": {
    "turbo": "1.10.7",
    "typescript": "5.1.6",
    "ts-node": "10.9.1",
    "@tsconfig/node18": "1.0.1",
    "@types/node": "20.5.3",
    "@babel/core": "7.22.10",
    "@babel/preset-env": "7.22.10",
    "@babel/preset-typescript": "7.22.5",
    "@types/jest": "29.5.4",
    "jest": "29.6.3",
    "@jest/globals": "^29.6.4",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/*",
        "typescript",
        "ts-node",
        "@tsconfig/*",
        "@types/*",
        "jest",
        "@jest/*",
        "supertest"
      ]
    }
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  }
}
