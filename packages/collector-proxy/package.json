{
  "name": "@graphql-debugger/collector-proxy",
  "version": "0.0.0-alpha.27",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "prisma"
  ],
  "scripts": {
    "build": "tsc && npm run generate-types",
    "dev": "DEBUG=\"@graphql-debugger:*\" TRACE_SCHEMA=\"true\" NODE_ENV=development ts-node ./src/main.ts",
    "test": "NODE_ENV=test jest",
    "generate-types": "ts-node ./src/graphql/generate && graphql-codegen --config codegen.ts",
    "postinstall": "npx prisma generate && npx prisma db push"
  },
  "dependencies": {
    "@graphql-debugger/trace-schema": "workspace:^",
    "@graphql-debugger/utils": "workspace:^",
    "@graphql-debugger/ui": "workspace:^",
    "@graphql-tools/schema": "^10.0.0",
    "@graphql-tools/utils": "^10.0.5",
    "@pothos/core": "^3.33.0",
    "@prisma/client": "^5.2.0",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "fastq": "^1.15.0",
    "graphql-yoga": "^4.0.4",
    "neo4j-driver": "^5.11.0",
    "prisma": "^5.2.0",
    "ts-node": "^10.9.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-resolvers": "4.0.1",
    "@jest/globals": "^29.6.4",
    "@opentelemetry/otlp-transformer": "^0.41.2",
    "@tsconfig/node18": "1.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.3",
    "@types/supertest": "^2.0.12",
    "gql-tag": "^1.0.1",
    "jest": "^29.6.3",
    "supertest": "^6.3.3",
    "typescript": "^5.1.6"
  }
}
